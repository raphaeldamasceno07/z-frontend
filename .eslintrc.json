{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "arrowParens": "always",
        "trailingComma": "none"
      }
    ]
  }
}
